<h2>Login</h2>

<form class="form login-form">
  <div class="form-group">
    <label for="email-login">email:</label>
    <input class="form-input email" type="text" id="email-login" />
  </div>
  <div class="form-group">
    <label for="password-login">password:</label>
    <input class="form-input password" type="password" id="password-login" />
  </div>
  <div class="form-group">
    <button class="btn btn-primary" id="login-btn" type="submit">login</button>
  </div>
</form>
<button class="btn btn-danger"> <a href="/newuser">Create Account </a></button>

<script src="/js/login.js"></script>
